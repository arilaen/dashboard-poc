{
  "name": "dashboard-poc",
  "description": "A dashboard for Cantina projects, with language and time statistics.",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/arilaen/dashboard-poc.git"
  },
  "homepage": "https://github.com/arilaen/dashboard-poc",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint . ./",
    "lint:fix": "npm run lint -- --fix",
    "dev": "npm-run-all --parallel development-client development-build-server development-server",
    "development-client": "node ./webpack/development.entry.js",
    "development-build-server": "webpack --verbose --colors --display-error-details --watch --config \"./webpack/development.build.entry.js\"",
    "development-page-server": "nodemon ./src/web-server/start.js --watch ./src/web-server/page-server.js --watch ./src/client --watch ./src/common --watch ./build/assets",
    "development-web-server": "nodemon ./src/web-server/entry.js --watch ./src/web-server/web-server.js --watch ./src/common",
    "development-server": "npm-run-all --parallel \"delay 500\" development-web-server \"delay 1000\" development-page-server",
    "production": "npm-run-all production-build-client production-server",
    "production-build-client": "webpack --verbose --colors --display-error-details --config \"webpack/production.build.entry.js\"",
    "production-web-server": "node ./src/web-server/entry.js --production",
    "production-server": "npm-run-all production-web-server"
  },
  "dependencies": {
    "babel-polyfill": "6.3.14",
    "bluebird": "^2.10.0",
    "body-parser": "^1.2.0",
    "bunyan": "^1.3.5",
    "bytes": "^2.1.0",
    "co-busboy": "^1.3.1",
    "colors": "^1.1.2",
    "compression": "^1.5.1",
    "cookie-parser": "^1.1.0",
    "cors": "^2.7.1",
    "crypto": "0.0.3",
    "debug": "^2.2.0",
    "deep-equal": "^1.0.1",
    "fs-extra": "^0.24.0",
    "history": "^1.13.1",
    "hoist-non-react-statics": "^1.0.3",
    "http-proxy": "^1.11.1",
    "is": "^3.0.1",
    "jspm": "^0.16.0",
    "kcors": "^1.0.1",
    "koa-body-parsers": "^1.1.0",
    "koa-bodyparser": "^2.0.1",
    "koa-bunyan": "0.0.2",
    "koa-compress": "^1.0.8",
    "koa-graphql": "^0.2.0",
    "koa-locale": "^1.0.0",
    "koa-mount": "^1.3.0",
    "koa-passport": "1.2.0",
