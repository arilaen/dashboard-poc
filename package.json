    "passport-google-oauth": "^0.2.0",
    "passport-oauth2": "^1.1.2",
    "pretty-error": "^1.1.2",
    "query-string": "^3.0.0",
    "react": "^0.14.0",
    "react-document-meta": "^2.0.0",
    "react-dom": "^0.14.0",
    "react-isomorphic-render": "2.0.1",
    "react-redux": "^4.0.0",
    "react-relay": "^0.4.0",
